{
    "compilerOptions": {
        "target": "ES5",
        "lib": ["DOM","DOM.Iterable","ESNext"],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": false   
    }
}